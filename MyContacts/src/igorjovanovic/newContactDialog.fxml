<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="igorjovanovic.DialogController" >
    <headerText>
        Enter new Contact
    </headerText>
    <content>
        <GridPane vgap="5" hgap="5">
            <Label  text="First Name" GridPane.rowIndex="0" GridPane.columnIndex="0" />
            <TextField fx:id="firstName" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Last Name" GridPane.rowIndex="1" GridPane.columnIndex="0" />
            <TextField fx:id="lastName" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Phone Number" GridPane.rowIndex="2" GridPane.columnIndex="0" />
            <TextField fx:id="phoneNumber" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label text="Note" GridPane.rowIndex="3" GridPane.columnIndex="0" />
            <TextField fx:id="note" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        </GridPane>
    </content>




</DialogPane>
